{
  "name": "cantoserver",
  "version": "1.0.0",
  "description": "CantoTalk Server",
  "main": "script.js",
  "scripts": {
    "client": "cd client/ && npm start",
    "server": "nodemon server.js",
    "build": "cd client/ && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "keywords": [],
  "author": "Brian MacPherson",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.520.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ejs-promise": "^0.3.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "mailgun-js": "^0.22.0",
    "node-schedule": "^1.3.2",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1"
  }
}
